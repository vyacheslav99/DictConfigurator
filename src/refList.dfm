  object dsRefList: TpFIBDataSet
    SelectSQL.Strings = (
      'select r.PK, r.DESCRIPTOR_, r.TITLE, r.BASE_DESCRIPTOR,'
      
        '  (case when o.JOIN_PARENT_PK is null then 1 else 0 end) MAIN_OB' +
        'JECT,'
      '  d.NAME FOLDER, u.LOGIN, u.NAME USER_NAME'
      'from DYNAMIC_FORM_OBJECT_TREE o'
      '  join DYNAMIC_FORM_REFERENCE r on r.MAIN_FORM_PK = o.FORM_PK'
      '  join USERS u on u.PK = r.OWNER_USER_PK'
      '  join DYNAMIC_FORM_FOLDER d on d.PK = r.FOLDER_PK'
      'where o.NAME = :NAME')
