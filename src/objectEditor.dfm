  object dsJoinParent: TpFIBDataSet
    SelectSQL.Strings = (
      'select PK, NAME, FORM_PK, JOIN_PARENT_PK from ('
      
        '  select PK, NAME, FORM_PK, JOIN_PARENT_PK, 2 as ORD from DYNAMI' +
        'C_FORM_OBJECT_TREE'
      '  where FORM_PK = :FORM_PK'
      '  union'
      
        '  select o.PK, o.NAME, o.FORM_PK, o.JOIN_PARENT_PK, 1 as ORD fro' +
        'm DYNAMIC_FORM_REFERENCE r'
      
        '    join DYNAMIC_FORM_OBJECT_TREE o on o.FORM_PK = r.MAIN_FORM_P' +
        'K'
      
        '  where r.PK = :PARENT_PK and (JOIN_PARENT_PK is null or (JOIN_P' +
